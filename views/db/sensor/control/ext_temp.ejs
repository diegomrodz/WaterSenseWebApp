<div class="row" style="margin-top: 20px;">
  <h3 style="text-transform: uppercase;">{{ sensor.nickname }} <small style="float: right;">Temperatura Externa</small></h3>
</div>

<div class="h-10"></div>

<div style="width: 100%; height: 487px; padding-top: 15px; margin-bottom: 20px;" class="col-sm-12 col-xs-12 col-md-12 invoice">
  <h5>Gráfico Diário</h5>

  <div class="h-10"></div>

  <chartist class="ct-chart" chartist-chart-options="chartOptions" chartist-data="dailyExtTempDataset" chartist-chart-type="Line"></chartist>    
</div>


<div style="width: 100%; height: 487px; padding-top: 15px;" class="col-sm-12 col-xs-12 col-md-12 invoice">
  <h5>Gráfico Semanal</h5>

  <div class="h-10"></div>

  <chartist class="ct-chart" chartist-chart-options="chartOptions" chartist-data="weeklyExtTempDataset" chartist-chart-type="Line"></chartist>    
</div>

